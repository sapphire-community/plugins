{
	"name": "@sapphire/plugin-api",
	"version": "0.0.1",
	"description": "Plugin for @sapphire/framework to expose a REST API",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./register": {
			"import": "./dist/register.mjs",
			"require": "./dist/register.js"
		}
	},
	"homepage": "https://github.com/sapphire-project/plugins/tree/main/packages/api",
	"scripts": {
		"test": "jest",
		"lint": "eslint src tests --ext ts --fix",
		"build": "tsc -b src",
		"postbuild": "run-p esm:**",
		"esm:register": "gen-esm-wrapper dist/register.js dist/register.mjs",
		"esm:default": "gen-esm-wrapper dist/index.js dist/index.mjs"
	},
	"dependencies": {
		"@sapphire/utilities": "^1.1.1"
	},
	"peerDependencies": {
		"@sapphire/framework": "github:sapphire-project/framework#build",
		"@sapphire/pieces": "github:sapphire-project/pieces#build",
		"@types/ws": "latest",
		"discord.js": "12.x"
	},
	"peerDependenciesMeta": {
		"@types/ws": {
			"optional": true
		}
	},
	"devDependencies": {},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphire-project/plugins.git",
		"directory": "packages/api"
	},
	"files": [
		"dist",
		"!dist/tsconfig.tsbuildinfo"
	],
	"engines": {
		"node": ">=12",
		"npm": ">=6"
	},
	"keywords": [
		"sapphire-project",
		"plugin",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire"
	],
	"bugs": {
		"url": "https://github.com/sapphire-project/plugins/issues"
	},
	"publishConfig": {
		"access": "public"
	}
}
