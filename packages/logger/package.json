{
	"name": "@sapphire/plugin-logger",
	"version": "1.0.5",
	"description": "Plugin for @sapphire/framework to have pretty console output",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./register": {
			"import": "./register.mjs",
			"require": "./register.js"
		}
	},
	"homepage": "https://github.com/sapphire-project/plugins/tree/main/packages/logger",
	"scripts": {
		"test": "jest",
		"lint": "eslint src tests --ext ts --fix",
		"build": "tsc -b src",
		"postbuild": "run-p esm:**",
		"esm:register": "gen-esm-wrapper dist/register.js dist/register.mjs",
		"esm:default": "gen-esm-wrapper dist/index.js dist/index.mjs",
		"prepublishOnly": "yarn build"
	},
	"dependencies": {
		"@sapphire/time-utilities": "^1.2.7",
		"colorette": "^1.2.1",
		"tslib": "^2.1.0"
	},
	"peerDependencies": {
		"@sapphire/framework": "1.x"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphire-project/plugins.git",
		"directory": "packages/logger"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo",
		"register.*"
	],
	"engines": {
		"node": ">=14",
		"npm": ">=6"
	},
	"keywords": [
		"sapphire-project",
		"plugin",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire"
	],
	"bugs": {
		"url": "https://github.com/sapphire-project/plugins/issues"
	},
	"publishConfig": {
		"access": "public"
	}
}
